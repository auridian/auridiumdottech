body{background-color:#0a0a0f;color:#fff;font-family:inter,sans-serif}.neon-text,.content-page h1,.section-title,.banner h1{text-shadow:0 0 5px #05d9e8,0 0 10px #05d9e8,0 0 20px #05d9e8;color:#05d9e8}.neon-text-pink{text-shadow:0 0 5px #ff2a6d,0 0 10px #ff2a6d,0 0 20px #ff2a6d;color:#ff2a6d}.navbar{background-color:rgba(5,5,7,.95);backdrop-filter:blur(10px);border-bottom:1px solid rgba(5,217,232,.2)}.navbar .nav-link{color:#fff}.navbar .nav-link:hover{color:#05d9e8}.banner{background:linear-gradient(135deg,#050507 0%,#0a0a0f 100%);position:relative;overflow:hidden}.banner::before{content:'';position:absolute;top:0;left:0;right:0;bottom:0;background:radial-gradient(circle at 20% 20%,rgba(5,217,232,.1) 0%,transparent 50%),radial-gradient(circle at 80% 80%,rgba(255,42,109,.1) 0%,transparent 50%);z-index:1}.banner .banner-content{position:relative;z-index:2}.banner h1{font-size:3.5rem;font-weight:800;margin-bottom:1.5rem}.banner p{color:#b3b3b3;font-size:1.25rem;margin-bottom:2rem}.btn-primary{background:linear-gradient(45deg,#05d9e8,#b967ff);border:none;color:#fff;padding:.75rem 1.5rem;border-radius:4px;font-weight:600;transition:all .3s ease}.btn-primary:hover{transform:translateY(-2px);box-shadow:0 0 15px rgba(5,217,232,.5)}.btn-secondary{background:0 0;border:2px solid #05d9e8;color:#05d9e8;padding:.75rem 1.5rem;border-radius:4px;font-weight:600;transition:all .3s ease}.btn-secondary:hover{background:rgba(5,217,232,.1);transform:translateY(-2px)}.feature-card{background:rgba(5,5,7,.5);border:1px solid rgba(5,217,232,.2);border-radius:8px;padding:2rem;transition:all .3s ease}.feature-card:hover{transform:translateY(-5px);border-color:#05d9e8;box-shadow:0 0 20px rgba(5,217,232,.2)}.feature-card .icon{color:#05d9e8;font-size:2.5rem;margin-bottom:1rem}.feature-card h3{color:#fff;margin-bottom:1rem}.feature-card p{color:#b3b3b3}.section{padding:5rem 0}.section-title{margin-bottom:1rem}.section-subtitle{color:#b3b3b3;margin-bottom:3rem}.footer{background-color:#050507;border-top:1px solid rgba(5,217,232,.2);padding:4rem 0 2rem}.footer h4{color:#05d9e8;margin-bottom:1.5rem}.footer a{color:#b3b3b3;text-decoration:none;transition:color .3s ease}.footer a:hover{color:#05d9e8}.alpha-form{max-width:600px;margin:2rem auto;padding:2rem;background:rgba(5,5,7,.5);border:1px solid rgba(5,217,232,.2);border-radius:8px}.alpha-form .form-group{margin-bottom:1.5rem}.alpha-form .form-group label{display:block;margin-bottom:.5rem;color:#05d9e8;font-weight:500}.alpha-form .form-group input,.alpha-form .form-group textarea,.alpha-form .form-group select{width:100%;padding:.75rem;background:rgba(10,10,15,.8);border:1px solid rgba(5,217,232,.3);border-radius:4px;color:#fff;transition:all .3s ease}.alpha-form .form-group input:focus,.alpha-form .form-group textarea:focus,.alpha-form .form-group select:focus{outline:none;border-color:#05d9e8;box-shadow:0 0 10px rgba(5,217,232,.3)}.alpha-form .form-group textarea{resize:vertical;min-height:100px}.content-page{max-width:800px;margin:0 auto;padding:2rem}.content-page h1{font-size:2.5rem;margin-bottom:2rem}.content-page h2{color:#ff2a6d;margin:2rem 0 1rem;font-size:1.8rem}.content-page h3{color:#05d9e8;margin:1.5rem 0 1rem;font-size:1.4rem}.content-page p{margin-bottom:1.5rem;line-height:1.6}.content-page ul,.content-page ol{margin-bottom:1.5rem;padding-left:1.5rem}.content-page ul li,.content-page ol li{margin-bottom:.5rem}.content-page a{color:#05d9e8;text-decoration:none;transition:color .3s ease}.content-page a:hover{color:#ff2a6d}.content-page blockquote{border-left:4px solid #05d9e8;padding-left:1rem;margin:1.5rem 0;font-style:italic;color:#b3b3b3}.content-page code{background:rgba(5,5,7,.5);padding:.2rem .4rem;border-radius:3px;font-family:fira code,monospace}.content-page pre{background:rgba(5,5,7,.5);padding:1rem;border-radius:4px;overflow-x:auto;margin:1.5rem 0}.content-page pre code{background:0 0;padding:0}.feature-grid{display:grid;grid-template-columns:repeat(auto-fit,minmax(300px,1fr));gap:2rem;margin:3rem 0}.timeline{position:relative;padding:2rem 0}.timeline::before{content:'';position:absolute;left:50%;top:0;bottom:0;width:2px;background:linear-gradient(to bottom,#05d9e8,#ff2a6d)}.timeline .timeline-item{margin:2rem 0;position:relative}.timeline .timeline-item::before{content:'';position:absolute;left:50%;top:0;width:20px;height:20px;background:#05d9e8;border-radius:50%;transform:translateX(-50%)}.timeline .timeline-item .timeline-content{width:calc(50% - 30px);padding:1.5rem;background:rgba(5,5,7,.5);border:1px solid rgba(5,217,232,.2);border-radius:8px}.timeline .timeline-item .timeline-content h3{color:#05d9e8;margin-bottom:1rem}.timeline .timeline-item:nth-child(odd) .timeline-content{margin-left:auto}@media(max-width:768px){.timeline::before{left:0}.timeline .timeline-item::before{left:0}.timeline .timeline-item .timeline-content{width:calc(100% - 30px);margin-left:30px!important}.alpha-form{padding:1rem}.content-page{padding:1rem}.content-page h1{font-size:2rem}}@keyframes glow{0%{box-shadow:0 0 5px rgba(5,217,232,.5)}50%{box-shadow:0 0 20px rgba(5,217,232,.8)}100%{box-shadow:0 0 5px rgba(5,217,232,.5)}}.glow-effect{animation:glow 2s infinite}